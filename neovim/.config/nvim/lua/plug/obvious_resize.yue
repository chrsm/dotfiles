import "macros" as { $ }

export default {
  config: ->
    import "which-key" as wk

    wk.register {
      ["<C-K>"]:
        * ":ObviousResizeUp<CR>"
      ["<C-J>"]:
        * ":ObviousResizeDown<CR>"
      ["<C-H>"]:
        * ":ObviousResizeLeft<CR>"
      ["<C-L>"]:
        * ":ObviousResizeRight<CR>"
    }, { mode: "n", noremap: true, silent: true }
}

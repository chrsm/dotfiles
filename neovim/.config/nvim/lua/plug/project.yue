import "macros" as { $ }

export default {
  config: ->
    import "project_nvim" as prj
    import "telescope" as ts
    import "which-key" as wk

    prj.setup {}
    ts.load_extension "projects"

    wk.register {
      o:
        * "<CMD>Telescope projects<CR>"
        * "List git projects"
    }, { mode: "n", prefix: "<leader>", noremap: true }
    --$noremap "<leader>o", "<cmd>Telescope projects<CR>"
}
